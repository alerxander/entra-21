package Cargos;

public class Desenvolvedor extends cargo {

	
	public void ir()
	{
		if(nivel == 1) {
			System.out.println("Imposto de renda:  "+salario*0.04);
		}else if(nivel == 2) {
		System.out.println("Imposto de renda:  "+salario*0.05);
		}else {
			System.out.println("Imposto de renda:  "+salario*0.10);
		}
	}
}
